setenv EDITOR vim
setenv COLORSCHEME base16-tomorrow
setenv BASE16_SHELL $HOME/base16-shell/scripts/$COLORSCHEME.sh
prepath $HOME/.pyenv/shims
prepath $HOME/bin

alias l='ls -lF'
alias la='ls -laF'
alias b='echo -e "\a"'
alias j='jobs'
alias g='git'

if_exists_exec nvim alias vim='nvim'
if_exists_exec nvim setenv EDITOR nvim
if_exists_exec xdg-open alias open='xdg-open'
if_exists_eval fuck thefuck --alias
if_exists_source $HOME/.ssh/environment
if_exists_sh $BASE16_SHELL

